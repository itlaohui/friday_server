<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.laohui.mapper.UserMapper">
    <select id="selectUserList" resultType="net.laohui.pojo.User">
        SELECT * FROM f_users
        LIMIT ${size * (page - 1)} , ${size}
    </select>
</mapper>